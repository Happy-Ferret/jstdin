{
  "name": "jstdin",
  "version": "0.9.0",
  "description":
    "a better `node -p` that accepts stdin and can format and highlight output",
  "homepage": "https://github.com/busterc/jstdin",
  "author": {
    "name": "Buster Collings",
    "email": "busterc@gmail.com",
    "url": "https://about.me/buster"
  },
  "files": ["lib"],
  "main": "lib/index.js",
  "keywords": [
    "cli",
    "eval",
    "stdin",
    "node-p",
    "js",
    "json",
    "parse",
    "pretty-print",
    "jq",
    "jq.node",
    "fx",
    "npx"
  ],
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.18.2",
    "eslint-plugin-prettier": "^2.2.0",
    "execa": "^0.9.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lec": "^1.0.1",
    "lint-staged": "^4.2.1",
    "nsp": "^2.6.3",
    "prettier": "^1.7.0"
  },
  "scripts": {
    "prepublish": "lec lib/cli.js -c LF && nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest --coverage"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "repository": "busterc/jstdin",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "ISC",
  "bin": "lib/cli.js",
  "dependencies": {
    "cardinal": "^1.0.0",
    "get-stdin": "^5.0.1",
    "meow": "^3.7.0"
  }
}
